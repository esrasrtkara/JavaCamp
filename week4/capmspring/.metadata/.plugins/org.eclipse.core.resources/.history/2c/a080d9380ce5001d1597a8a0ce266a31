package kodlama.io.rentACar.entities.concretes;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Table(name="brands")
@Data
@AllArgsConstructor
@Entity
@NoArgsConstructor
public class Brand {
	
	private int id;
	private String name;
	
}
